pipeline {
  agent any
  stages {
    stage('Dev') {
      steps {
        echo 'I want  to Develop'
      }
    }

    stage('Build') {
      steps {
        echo 'i want to build'
      }
    }

    stage('Test') {
      steps {
        echo 'I want to test'
      }
    }

    stage('deploy') {
      steps {
        echo 'I want to deploy'
      }
    }

  }
}